{
  "compilerOptions": {
    "target": "es5",
    "baseUrl": ".",
    "declaration": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "lib": ["dom", "dom.iterable", "esnext"],
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true
  },
  "exclude": ["node_modules"],
  "extends": "./src/.umi/tsconfig.json",
  "include": ["src", "**/*.ts", "**/*.tsx", "src/types/*.d.ts"]
}
